{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2020",
        "lib": [
            "es2020"
        ],
        "types": [
            "node"
        ],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "noImplicitAny": false,
        "declaration": false,
        "noEmitOnError": false
    },
    "include": [
        "./*.ts"
    ],
    "ts-node": {
        "transpileOnly": true,
        "compilerOptions": {
            "module": "commonjs"
        }
    }
}